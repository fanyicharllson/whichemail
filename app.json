{
  "expo": {
    "name": "WhichEmail",
    "slug": "whichemail",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_v2.png",
    "scheme": "whichemail",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo_v3.png",
      "resizeMode": "contain",
      "backgroundColor": "#3b82f6"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fanyicharllson.whichemail",
      "backgroundColor": "#3b82f6"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#3b82f6",
        "foregroundImage": "./assets/images/logo_v2.png"
      },
      "package": "com.fanyicharllson.whichemail",
      "backgroundColor": "#3b82f6",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo_v2.png",
      "backgroundColor": "#3b82f6"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_v3.png",
          "imageWidth": 400,
          "resizeMode": "contain",
          "backgroundColor": "#3b82f6",
          "dark": {
            "image": "./assets/images/logo_v2.png",
            "backgroundColor": "#1e40af"
          }
        }
      ],
      "expo-secure-store",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow WhichEmail to use Face ID for secure password access"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}